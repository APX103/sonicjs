{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": "http://localhost:4200",
        "credentials": false,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": {
        "action": "deny"
      }
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubdomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": [
      {
        "name": "frontend",
        "paths": [
          "/"
        ],
        "params": "$!../server/themes/base"
      },
      {
        "name": "boostrap",
        "paths": [
          "/"
        ],
        "params": "$!../node_modules/bootstrap/dist"
      },
      {
        "name": "jquery",
        "paths": [
          "/"
        ],
        "params": "$!../node_modules/jquery/dist"
      },
      {
        "name": "popper",
        "paths": [
          "/js"
        ],
        "params": "$!../node_modules/popper.js/dist/umd"
      },
      {
        "name": "jsoneditor",
        "paths": [
          "/"
        ],
        "params": "$!../node_modules/jsoneditor/dist"
      },
      {
        "name": "tinymce",
        "paths": [
          "/js"
        ],
        "params": "$!../node_modules/tinymce"
      },
      {
        "name": "font-awesome-fonts",
        "paths": [
          "/fonts"
        ],
        "params": "$!../node_modules/font-awesome/fonts"
      },
      {
        "name": "font-awesome",
        "paths": [
          "/"
        ],
        "params": "$!../node_modules/font-awesome"
      },
      {
        "name": "page-builder",
        "paths": [
          "/"
        ],
        "params": "$!../server/page-builder"
      },
      {
        "name": "admin",
        "paths": [
          "/admin"
        ],
        "params": "$!../admin"
      },
      {
        "name": "adminScripts",
        "paths": [
          "/admin"
        ],
        "params": "$!../admin/dist/client"
      },
      {
        "name": "assets",
        "paths": [
          "/"
        ],
        "params": "$!../server/assets"
      },
      {
        "name": "formio",
        "paths": [
          "/"
        ],
        "params": "$!../node_modules/formiojs/dist"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}