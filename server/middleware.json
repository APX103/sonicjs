{
  "initial:before": {
    "loopback#favicon": {
      "params": "server/assets/img/favicon.ico"
    }
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session:before": {
    "cookie-parser": {
      "params": "${cookieSecret}"
    }
  },
  "auth": {
    "loopback#token": {}
  },
  "parse": {
    "body-parser#json": {},
    "body-parser#urlencoded": {
      "params": {
        "extended": true
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": [
      {
        "paths": [
          "/"
        ],
        "params": "$!../server/themes/base"
      },
      {
        "paths": [
          "/vendors"
        ],
        "params": "$!../server/assets/vendors"
      },
      {
        "paths": [
          "/"
        ],
        "params": "$!../server/page-builder"
      },
      {
        "paths": [
          "/admin"
        ],
        "params": "$!../admin"
      },
      {
        "paths": [
          "/admin"
        ],
        "params": "$!../admin/dist/client"
      },
      {
        "paths": [
          "/"
        ],
        "params": "$!../server/assets"
      },
      {
        "paths": [
          "/admin"
        ],
        "params": "$!../server/views/admin/assets"
      },
      {
        "paths": [
          "/css"
        ],
        "params": "$!../server/storage/css"
      },
      {
        "paths": [
          "/js"
        ],
        "params": "$!../server/storage/js"
      },
      {
        "paths": [
          "/files"
        ],
        "params": "$!../server/storage/files"
      },
      {
        "paths": [
          "/services"
        ],
        "params": "$!../server/services"
      },
      {
        "paths": [
          "/node_modules"
        ],
        "params": "$!../node_modules"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
